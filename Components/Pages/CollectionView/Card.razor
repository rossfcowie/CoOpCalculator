@using CoOpCalculator.Components.Pages.CollectionView;
<div>
    <h1>@i</h1>
    <div class="row">
        <div class="col-4">
    @if(level.Contains("Add")){
        <FilledStar/>
    }else{
        <EmptyStar/>
    }
    </div>
        <div class="col-4">
    @if (level.Contains("Mul"))
    {
        <FilledStar/>
    }else{
        <EmptyStar/>
        }
    </div>
        <div class="col-4">
    @if (level.Contains("Sub"))
    {
        <FilledStar/>
    }else{
        <EmptyStar/>
    }
</div>
    </div>
</div>

@code {
    [Parameter]
    public string level { get; set; }
    [Parameter]
    public int i { get; set; }

}
